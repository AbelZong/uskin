{
  "name": "uskin",
  "description": "A graceful framework which provides developers another chance to build an amazing site.",
  "version": "0.1.5",
  "keywords": [
    "css",
    "less",
    "react",
    "babel",
    "front-end",
    "framework"
  ],
  "homepage": "https://github.com/icecreamliker/uskin",
  "author": "Lee Yao <yaoli111144@gmail.com>",
  "main": "index.js",
  "scripts": {
    "build": "grunt build",
    "release": "grunt release",
    "prepublish": "grunt build",
    "test": "jest",
    "eslint": "eslint *.js __tests__/ js/** docs/react_demos/** --ext '.js,.jsx'",
    "dev": "rm -rf dist && webpack --config dev.webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icecreamliker/uskin.git"
  },
  "bugs": {
    "url": "https://github.com/icecreamliker/uskin/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.1.18",
    "babel-core": "^6.1.4",
    "babel-eslint": "^5.0.0-beta4",
    "babel-jest": "6.0.1",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "babel-runtime": "^6.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "grunt": "^0.4.4",
    "grunt-banner": "^0.5.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.8.1",
    "grunt-jest": "^0.2.0",
    "grunt-webfont": "^1.1.0",
    "grunt-webpack": "^1.0.11",
    "jest-cli": "^0.7.1",
    "json-loader": "^0.5.3",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "lesslint": "^0.1.8-beta.1",
    "load-grunt-tasks": "^3.3.0",
    "postcss-loader": "^0.8.2",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "stylelint": "^1.0.1",
    "time-grunt": "^1.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.12.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest/index.js",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true
  }
}
